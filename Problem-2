
import java.util.Scanner;

public class Pblm2 {
	// we are taking a method called as generateSeries to generate the numbers in series as per condition
    public static void generateSeries(int a) {
        for (int i = 1; i <= a; i=i+1) {
            if (i % 2 != 0) {
                System.out.print(i + " ");
            }
        }
    }

    public static void main(String[] args) {
    	Scanner sc=new Scanner(System.in);
    	System.out.println("enter the value of your choice->");
        int a =sc.nextInt();
        int x=2*a-1;
        System.out.print("Output is: ");
        generateSeries(x);
    }
  
}
